// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
  todos     Todo[]    @relation("Todo_author")
}

model Todo {
  id             String  @id @default(cuid())
  title          String  @default("")
  description    Json    @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  priority       Int     @default(1)
  author         User?   @relation("Todo_author", fields: [authorId], references: [id])
  authorId       String? @map("author")
  from_Tag_todos Tag[]   @relation("Tag_todos")

  @@index([authorId])
}

model Tag {
  id    String @id @default(cuid())
  Name  String @unique @default("")
  todos Todo[] @relation("Tag_todos")
}
